{
    "name": "zizaco/entrust",
    "description": "This plugin provides a flexible way to add Role-based Permissions to Laravel 4",
    "keywords": ["laravel","illuminate","auth","roles"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.0.x",
        "laravelbook/ardent": "*"
    },
    "require-dev": {
        "illuminate/database": "4.0.x",
        "mockery/mockery": "0.7.2",
        "zizaco/factory-muff": "dev-master"
    },
    "suggest": {
        "zizaco/confide":"Confide is an authentication solution for Laravel 4 that couples very well with Entrust"
    },
    "autoload": {
        "psr-0": {
            "Zizaco\\Entrust": "src/"
        }
    },
    "minimum-stability": "dev"
}
